{
    "tags": {
        "allowUnknownTags": [
            "pailhead",
            "component",
            "component"
        ] 
    },
    "recurseDepth": 10,
    "opts": {
      "template": "node_modules/better-docs",
      "destination": "docs/jsdocs"
    },
    "source": {
        "include": ["./src/"],
        "excludePattern": ".*/(utils|MlFeatureEditor\/lib)/.*"
    },
    "plugins": [
        "node_modules/better-docs/component"
    ]
}
